{
    "version": 2,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "ram-mode": "SMALL",
        "rom-mode": "LARGE"
    },
    "c/cpp-compiler": {
        "optimization-type": "SIZE",
        "optimization-level": "level-9"
    },
    "asm-compiler": {},
    "linker": {
        "remove-unused": true,
        "output-format": "elf",
        "use-RTX": "None",
        "disable-warnings": [
            "16"
        ]
    }
}